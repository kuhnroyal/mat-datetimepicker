{
  "name": "mat-datetimepicker",
  "version": "7.0.1",
  "license": "MIT",
  "scripts": {
    "build": "ng build",
    "build:full": "npm run build:lib && ng build --configuration production",
    "build:lib": "npm run build:lib:core && npm run build:lib:moment",
    "build:lib:core": "ng build @mat-datetimepicker/core --configuration production && copy-files-from-to --config projects/core/copy-files.json",
    "build:lib:moment": "ng build @mat-datetimepicker/moment --configuration production && copy-files-from-to --config projects/moment/copy-files.json",
    "lint": "ng lint",
    "prepare": "owl install",
    "prettier:check": "prettier --check \"{,src/**/,projects/**/}*.{html,js,ts,css,scss}\"",
    "prettier:format": "prettier --write \"{,,src/**/,projects/**/}*.{html,js,ts,css,scss}\"",
    "release": "standard-version",
    "start": "ng serve",
    "test": "ng test",
    "test:headless": "ng test --browsers ChromeHeadless",
    "test:watch": "ng test --watch"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.0",
    "@angular/common": "~12.2.0",
    "@angular/compiler": "~12.2.0",
    "@angular/core": "~12.2.0",
    "@angular/forms": "~12.2.0",
    "@angular/material": "^12.2.6",
    "@angular/material-moment-adapter": "^12.2.6",
    "@angular/platform-browser": "~12.2.0",
    "@angular/platform-browser-dynamic": "~12.2.0",
    "@angular/router": "~12.2.0",
    "moment": "^2.29.1",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.6",
    "@angular-eslint/builder": "12.4.1",
    "@angular-eslint/eslint-plugin": "12.4.1",
    "@angular-eslint/eslint-plugin-template": "12.4.1",
    "@angular-eslint/schematics": "12.4.1",
    "@angular-eslint/template-parser": "12.4.1",
    "@angular/cli": "~12.2.6",
    "@angular/compiler-cli": "~12.2.0",
    "@bbortt/owl": "^1.0.2",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "4.28.2",
    "@typescript-eslint/parser": "4.28.2",
    "copy-files-from-to": "^3.2.1",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ng-packagr": "^12.1.1",
    "prettier": "^2.4.1",
    "pretty-quick": "^3.1.1",
    "standard-version": "^9.3.1",
    "typescript": "~4.3.5"
  }
}
