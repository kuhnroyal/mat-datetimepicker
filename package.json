{
  "name": "mat-datetimepicker",
  "version": "6.0.3",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:prod": "ng serve --configuration production",
    "build": "yarn build:lib && ng build",
    "build:prod": "yarn build:lib && ng build --configuration production --stats-json",
    "build:lib": "yarn build:lib/core && yarn build:lib/moment",
    "build:lib/core": "ng build core --configuration production && copy-files-from-to --config projects/core/copy-files.json",
    "build:lib/moment": "ng build moment --configuration production && copy-files-from-to --config projects/moment/copy-files.json",
    "bundle-report": "webpack-bundle-analyzer dist/app/stats.json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.1.2",
    "@angular/cdk": "~12.1.2",
    "@angular/common": "^12.1.2",
    "@angular/compiler": "^12.1.2",
    "@angular/core": "^12.1.2",
    "@angular/forms": "^12.1.2",
    "@angular/material": "~12.1.2",
    "@angular/material-moment-adapter": "~12.1.2",
    "@angular/platform-browser": "^12.1.2",
    "@angular/platform-browser-dynamic": "^12.1.2",
    "@angular/router": "^12.1.2",
    "copy-files-from-to": "3.2.0",
    "moment": "2.29.1",
    "rxjs": "~7.2.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.1.2",
    "@angular/cli": "^12.1.2",
    "@angular/compiler-cli": "^12.1.2",
    "@angular/language-service": "^12.1.2",
    "@types/jasmine": "~3.8.1",
    "@types/jasminewd2": "^2.0.10",
    "@types/node": "^16.3.2",
    "codelyzer": "^6.0.2",
    "jasmine-core": "~3.8.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.7.0",
    "ng-packagr": "^12.1.2",
    "protractor": "~7.0.0",
    "rollup": "^2.53.2",
    "ts-node": "~10.1.0",
    "tslint": "~6.1.3",
    "typescript": "4.3.5",
    "webpack-bundle-analyzer": "4.4.2"
  }
}
