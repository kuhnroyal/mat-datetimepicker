{
  "name": "mat-datetimepicker",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "npm run build:lib && ng build",
    "build:prod": "npm run build:lib && ng build --configuration production --stats-json",
    "build:lib": "npm run build:lib/core && npm run build:lib/moment && copy-files-from-to --config copy-files.json",
    "build:lib/core": "ng build core --configuration production",
    "build:lib/moment": "ng build moment --configuration production",
    "build:pages": "ng build --base-href=/mat-datetimepicker/ --configuration production",
    "bundle-report": "webpack-bundle-analyzer dist/app/stats.json",
    "lint": "ng lint",
    "publish:pages": "ng deploy",
    "prepare": "husky install",
    "prettier": "prettier \"{,e2e/**/,projects/**/,src/**/}*.{md,java,json,y(a)?ml,html,js,ts,tsx,css,scss}\"",
    "prettier:check": "npm run prettier -- --check",
    "prettier:fix": "npm run prettier -- --write",
    "start": "ng serve",
    "start:prod": "ng serve --configuration production",
    "test": "ng test",
    "test:watch": "ng test --auto-watch --no-single-run",
    "watch:lib/core": "ng build core --watch",
    "watch:lib/moment": "ng build moment --watch"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.2",
    "@angular/cdk": "^15.2.2",
    "@angular/common": "^15.2.2",
    "@angular/compiler": "^15.2.2",
    "@angular/core": "^15.2.2",
    "@angular/forms": "^15.2.2",
    "@angular/material": "^15.2.2",
    "@angular/material-moment-adapter": "^15.2.2",
    "@angular/platform-browser": "^15.2.2",
    "@angular/platform-browser-dynamic": "^15.2.8",
    "@angular/router": "^15.2.2",
    "copy-files-from-to": "3.8.0",
    "moment": "2.29.4",
    "rxjs": "^7.8.0",
    "tslib": "^2.5.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.5",
    "@angular-eslint/builder": "15.2.1",
    "@angular-eslint/eslint-plugin": "15.2.1",
    "@angular-eslint/eslint-plugin-template": "15.2.1",
    "@angular-eslint/schematics": "15.2.1",
    "@angular-eslint/template-parser": "15.2.1",
    "@angular/cli": "^15.2.2",
    "@angular/compiler-cli": "^15.2.2",
    "@angular/language-service": "^15.2.2",
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.7",
    "@semantic-release/npm": "^9.0.2",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@types/jasmine": "~4.3.1",
    "@types/jasminewd2": "^2.0.10",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.57.1",
    "angular-cli-ghpages": "^1.0.5",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jasmine-core": "~4.5.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.4.1",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.0.0",
    "ng-packagr": "^15.2.2",
    "prettier": "^2.8.4",
    "pretty-quick": "^3.1.3",
    "semantic-release": "^20.1.1",
    "ts-node": "~10.9.1",
    "typescript": "^4.9",
    "webpack-bundle-analyzer": "4.8.0"
  },
  "engines": {
    "node": ">=18.15.0 "
  }
}
