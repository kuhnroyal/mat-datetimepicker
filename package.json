{
  "name": "mat-datetimepicker",
  "version": "5.0.1",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:prod": "ng serve --prod",
    "build": "yarn build:lib && ng build",
    "build:prod": "yarn build:lib && ng build --prod --stats-json",
    "build:lib": "yarn build:lib/core && yarn build:lib/moment",
    "build:lib/core": "ng build core --prod && copy-files-from-to --config projects/core/copy-files.json",
    "build:lib/moment": "ng build moment --prod && copy-files-from-to --config projects/moment/copy-files.json",
    "bundle-report": "webpack-bundle-analyzer dist/app/stats.json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^10.1.4",
    "@angular/cdk": "~10.1.1",
    "@angular/common": "^10.1.4",
    "@angular/compiler": "^10.1.4",
    "@angular/core": "^10.1.4",
    "@angular/forms": "^10.1.4",
    "@angular/material": "~10.1.1",
    "@angular/material-moment-adapter": "~10.1.1",
    "@angular/platform-browser": "^10.1.4",
    "@angular/platform-browser-dynamic": "^10.1.4",
    "@angular/router": "^10.1.4",
    "copy-files-from-to": "3.1.4",
    "moment": "2.27.0",
    "rxjs": "~6.6.2",
    "tsickle": "^0.39.1",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1001.4",
    "@angular-devkit/build-ng-packagr": "~0.1001.4",
    "@angular/cli": "^10.1.4",
    "@angular/compiler-cli": "^10.1.4",
    "@angular/language-service": "^10.1.4",
    "@types/jasmine": "^3.5.12",
    "@types/jasminewd2": "^2.0.6",
    "@types/node": "^14.0.27",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^10.0.0",
    "protractor": "~7.0.0",
    "rollup": "^2.23.0",
    "ts-node": "~8.10.2",
    "tslint": "~6.1.0",
    "typescript": "3.9.7",
    "webpack-bundle-analyzer": "3.8.0"
  }
}